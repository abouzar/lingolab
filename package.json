{
  "name": "lingolab",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:full": "run-p dev dev:server",
    "server:build": "tsc --project server/tsconfig.json",
    "server:start": "node dist/server/index.js",
    "test:unit": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "test:pipeline": "node scripts/test-pipeline.js",
    "test:quick": "node scripts/quick-test.js",
    "test:pre-commit": "node scripts/pre-commit-test.js",
    "test:db-health": "node scripts/db-health-check.js"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "pinia": "^2.1.7",
    "prisma": "^6.13.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.4.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  }
}
